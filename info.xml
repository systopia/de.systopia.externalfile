<?xml version="1.0"?>
<extension key="de.systopia.externalfile" type="module">
  <file>externalfile</file>
  <name>de.systopia.externalfile</name>
  <description>This Extension provides an API to manage external files.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Benni Baermann</author>
    <email>baermann@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.externalfile</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.externalfile</url>
    <url desc="Support">https://github.com/systopia/de.systopia.externalfile</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-12-28</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.56</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Externalfile</namespace>
    <format>22.12.1</format>
    <angularModule>crmExternalfile</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Externalfile_Upgrader</upgrader>
</extension>
